<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <link rel="stylesheet" href="css/login.css">
    <title>THE BUTTON</title>
</head>
<body>
    
    <form action="/changeUsername?_method=PUT" method="POST">
        <input type="hidden" name="_method" value="put" />
        <input id="text-input" type="input" name="username" placeholder="Username">
        <div id="buttonDiv" class="noclick">
            <input id="username-submit-button" type="submit" disabled>
            <div class="button-bottom-shadow absolute rounded"></div>
            <div class="button-bottom-shadow-rect absolute"></div>
            <div class="button-bottom absolute rounded"></div>
            <div class="button-click-bottom absolute rounded"></div>
            <div class="button-click-middle-rect absolute"></div>
            <div class="button-click absolute rounded "></div>
        </div>
        
        <script src="js/login.js"></script>
    </form>
</body>
</html>